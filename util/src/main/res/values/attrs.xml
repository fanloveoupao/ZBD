<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotProgressBar">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="amount" format="integer"/>
        <attr name="animationDirection" format="enum">
            <enum name="right" value="1"/>
            <enum name="left" value="-1"/>
        </attr>
    </declare-styleable>
    <!--九宫格图片-->
    <declare-styleable name="NineGridImageView">
        <attr name="singleImgSize" format="dimension"/>
        <attr name="imgSize" format="dimension"/>
        <attr name="imgGap" format="dimension"/>
        <attr name="maxSize" format="integer"/>
        <attr name="showStyle" format="enum">
            <enum name="grid" value="0"/>
            <enum name="fill" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TagCloudView">
        <attr name="tcvBackground" format="reference"/>
        <attr name="tcvTextColor" format="reference"/>
        <attr name="tcvBorder" format="dimension"/>
        <attr name="tcvTextSize" format="dimension"/>
        <attr name="tcvBorderItem" format="dimension"/>
        <attr name="tcvItemBorderVertical" format="dimension"/>
        <attr name="tcvItemBorderHorizontal" format="dimension"/>
        <attr name="tcvTextPaddingHorizontal" format="dimension"/>
        <attr name="tcvTextPaddingVertical" format="dimension"/>
        <attr name="tcvSingleLine" format="boolean"/>
        <attr name="tcvShowEndText" format="boolean"/>
        <attr name="tcvShowRightImg" format="boolean"/>
        <attr name="tcvEndText" format="string"/>
        <attr name="tcvRightResId" format="reference"/>
        <attr name="tcvCanTagClick" format="boolean"/>

        <attr name="tcvTagResId" format="reference"/>
    </declare-styleable>

    <!--聊天里面的圆角-->
    <attr name="borderRadius" format="dimension"/>
    <attr name="type">
        <enum name="circle" value="0"/>
        <enum name="round" value="1"/>
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius"/>
        <attr name="type"/>
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="PraiseListView">
        <attr name="item_color" format="color"/>
        <attr name="item_selector_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="StrokeTextView">
        <attr name="textColor" format="color|reference"/>
        <attr name="outerColor" format="color|reference"/>
        <attr name="width" format="integer"/>
    </declare-styleable>
    <declare-styleable name="MultilineEditText">
        <attr name="max_count" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="PileLayout">
        <attr name="PileLayout_vertivalSpace" format="dimension"/>
        <attr name="PileLayout_pileWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="AbsRoundImageView">
        <attr name="round_borderWidth" format="dimension|reference"/>
        <attr name="round_borderColor" format="color|reference"/>
    </declare-styleable>


    <declare-styleable name="TimeLineView">
        <attr name="timelineRadius" format="dimension"/>
        <attr name="timelineHeadRadius" format="dimension"/>
        <attr name="timelineHeadColor" format="color"/>
        <attr name="timelineOtherColor" format="color"/>
        <attr name="timelineRadiusDistance" format="dimension"/>
        <attr name="timelineCount" format="integer"/>
        <attr name="timelineMarginTop" format="dimension"/>
        <attr name="timelineWidth" format="dimension"/>
        <attr name="timelinefirstImg" format="reference"/>
    </declare-styleable>


</resources>